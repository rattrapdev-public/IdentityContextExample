@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Client Search</title>
    <link rel="stylesheet" type="text/css" href="/Content/bootstrap.css">
</head>
<body >
	<div class="container">
		<nav class="navbar navbar-default" role="navigation">
			<div class="navbar-header">
				<a class="navbar-brand" href="#">Menu</a>
			</div>
			<div>
				<ul class="nav navbar-nav">
					<li><a href="/admin">Home</a></li>
					<li><a href="/admin/clients" >Clients</a></li>
					<li class="active"><a href="/admin/apps" >Apps</a></li>
					<li><a href="/admin/users">Users</a></li>
					<li><a href="/admin/permissions">Permissions</a></li>
				</ul>
			</div>
		</nav>
		<div id="site-content">
			<h2>Client Search</h2>
			@Html.Partial("Views/Admin/Partials/Validation")
			<table class="table table-bordered table-striped">
				<caption>Viewing All Apps</caption>
				<thead>
					<tr>
						<th>App Name</th>
					</tr>
				</thead>
				<tbody>
				@foreach(var app in Model) 
				{
					<tr>
						<td><a href="/admin/apps/@app.ApplicationId">@app.Name</a></td>
					</tr>
				}
				</tbody>
			</table>
		</div>
		<div>
			<a href="/admin/apps/new">Create New App</a>
		</div>
	</div>
</body>
</html>